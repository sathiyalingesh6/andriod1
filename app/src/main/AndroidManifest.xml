        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExpenseTracker"
        tools:targetApi="31">
        <activity
            android:name="com.example.LoginActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
            android:exported="true" />
        <activity
            android:name="com.example.Update_delete_Transactions"
            android:exported="true" />
        <activity
            android:name="com.example.AddTransactionActivity"
            android:exported="true" />
        <activity
            android:name="com.example.pie_charttt"
            android:exported="true" />
        <activity
            android:name="com.example.MainActivity"
            android:exported="true">

        </activity>


    </application>
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />

</manifest>